<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="luma test">
    <parameter name="useCloudEnv" value="false"></parameter>

    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://magento.softwaretestingboard.com/"></parameter>
    <test name="luma tests">
        <classes>

            <class name="tests.magento.CreateAnAccountTest"></class>
            <class name="tests.magento.SignIn"></class>
            <class name="tests.magento.ForgotPassword"></class>
            <class name="tests.magento.TestSearch"></class>
            <class name="tests.magento.PurchaseTest"></class>
            <class name="tests.magento.AddToCart"></class>
<!--            <class name="test.MyAccountTest"></class>-->
<!--            <class name="test.SignOut"></class>-->

        </classes>
    </test>
</suite>